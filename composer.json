{
    "type": "project",
    "license": "proprietary",
    "require": {
        "behat/behat": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Domain\\": "src/Domain/",
            "App\\": "src/App/",
            "Infra\\": "src/Infra/"
        }
    }
}
